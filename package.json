{
    "name": "react-redux-boilerplate",
    "version": "1.0.0",
    "description": "Boilerplate for React+Redux webapps",
    "main": "webpack.config.dev.js",
    "dependencies": {
        "extensible-duck": "^1.4.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-redux": "^5.0.7",
        "redux": "^3.7.2",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.2.0"
    },
    "scripts": {
        "build": "webpack --config webpack.config.js --verbose",
        "build:dev": "webpack --config webpack.config.js --verbose --env=dev",
        "dev": "webpack-dev-server --env=dev --watch --open",
        "debug": "yarn build -- --env=1",
        "prod": "yarn build -- --env=prod",
        "webpack-dev-server": "webpack-dev-server"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/shashankKeshava/react-redux-boilerplate.git"
    },
    "keywords": [
        "boilerplate"
    ],
    "author": "Shashank k <keshavashashank@gmail.com>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/shashankKeshava/react-redux-boilerplate/issues"
    },
    "homepage": "https://github.com/shashankKeshava/react-redux-boilerplate#readme",
    "devDependencies": {
        "babel": "^6.23.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "html-webpack-plugin": "^2.30.1",
        "transform-runtime": "^0.0.0",
        "webpack": "^3.10.0",
        "webpack-dev-server": "^2.11.1",
        "webpack-merge": "^4.1.1"
    }
}
